repeat wait() until game:IsLoaded();
if game.PlaceId == 2753915549 or game.PlaceId == 4442272183 or game.PlaceId == 7449423635 then
    if _G.KAITUN_SCRIPT then
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Zpshub/SnowXhub/main/Paid-Kaitun"))()
    else
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Zpshub/SnowXhub/main/Paid-Main"))()
    end
else
    game.Players.LocalPlayer:Kick("Hub Not Support Game")
    wait(1)
    game:Shutdown()
end
